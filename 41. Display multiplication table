using System;

class Program
{
    static void Main()
    {
        // Input the number for which the multiplication table is to be displayed
        Console.Write("Enter a number to display its multiplication table: ");
        int number = Convert.ToInt32(Console.ReadLine());

        // Display the multiplication table for the entered number
        Console.WriteLine($"Multiplication table for {number}:");
        for (int i = 1; i <= 10; i++) // Loop through numbers 1 to 10
        {
            Console.WriteLine($"{number} x {i} = {number * i}");
        }
    }
}
